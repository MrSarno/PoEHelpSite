hrome",[251,277,279],{"id":278},"edge","Edge",[256,281,282,287,291],{},[259,283,261,284,266],{},[263,285,286],{},"edge://settings/system/manageSystem#GraphicsAcceleration&1",[259,288,269,289],{},[263,290,272],{},[259,292,293],{},"Restart Edge",[251,295,297],{"id":296},"firefox","Firefox",[256,299,300,305,308,314],{},[259,301,261,302,266],{},[263,303,304],{},"about:preferences",[259,306,307],{},"Scroll down to the \"Performance\" section",[259,309,310,311],{},"Untick ",[263,312,313],{},"Use hardware acceleration when available",[259,315,316],{},"Restart Firefox",[251,318,320],{"id":319},"opera","Opera",[256,322,323,328,332],{},[259,324,261,325,266],{},[263,326,327],{},"opera://settings/?search=acceleration",[259,329,269,330],{},[263,331,272],{},[259,333,334],{},"Restart Opera",[251,336,338],{"id":337},"orion","Orion",[340,341,342],"warning",{},[233,343,344],{},"There is no known method of doing this in Orion. As a Sarfari-based browser, it likely lacks the option.",[251,346,348],{"id":347},"safari","Safari",[350,351,352],"caution",{},[233,353,354],{},"In macOS 10.15 Catalina the option to disable Hardware Acceleration was removed from Safari.",[251,356,358],{"id":357},"vivaldi","Vivaldi",[256,360,361,366,371],{},[259,362,261,363,266],{},[263,364,365],{},"vivaldi:settings/webpages/",[259,367,310,368],{},[263,369,370],{},"Use Hardware Acceleration when Available",[259,372,373],{},"Restart Vivaldi",{"title":375,"searchDepth":376,"depth":376,"links":377},"",2,[378],{"id":248,"depth":376,"text":249,"children":379},[380,382,383,384,385,386,387],{"id":253,"depth":381,"text":254},3,{"id":278,"depth":381,"text":279},{"id":296,"depth":381,"text":297},{"id":319,"depth":381,"text":320},{"id":337,"depth":381,"text":338},{"id":347,"depth":381,"text":348},{"id":357,"depth":381,"text":358},"Preventing web browsers from competing with PoE for your GPU's resources","md",{},{"icon":18},{"title":188,"description":393},"Preventing web browsers from competing with PoE for your GPU's resou